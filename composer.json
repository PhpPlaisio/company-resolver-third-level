{
  "name": "plaisio/company-resolver-third-level",
  "description": "PhpPlaisio: Third Level Company Resolver",
  "keywords": [
    "PhpPlaisio",
    "Company Resolver"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "plaisio/company-resolver": "^1.1",
    "plaisio/kernel": "^2.0"
  },
  "require-dev": {
    "phing/phing": "^2.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\CompanyResolver\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\CompanyResolver\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
