{
  "name": "plaisio/company-resolver-third-level",
  "description": "PhpPlaisio: Third Level Company Resolver",
  "keywords": [
    "PhpPlaisio",
    "Company Resolver"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "plaisio/canonical-hostname-resolver": "^1.2",
    "plaisio/company-resolver": "^2.0",
    "plaisio/kernel": "^3.0.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phing/phing": "^3.0.0-RC4",
    "phpunit/phpunit": "^9.6.2",
    "plaisio/console-kernel": "^1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\CompanyResolver\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\CompanyResolver\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
